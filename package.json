{
  "name": "tcg",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next start",
    "db:check": "bun run --bun drizzle-kit check",
    "db:drop": "bun run --bun drizzle-kit drop",
    "db:drop-all": "bun run --bun scripts/drop-db.ts",
    "db:generate": "bun run --bun drizzle-kit generate",
    "db:migrate": "bun run --bun drizzle-kit migrate",
    "db:pull": "bun run --bun drizzle-kit introspect",
    "db:studio": "bun run --bun drizzle-kit studio",
    "db:up": "bun run --bun drizzle-kit up",
    "db:seed": "bun run --bun scripts/seed.ts",
    "format:check": "bun run --bun prettier --check \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:write": "bun run --bun prettier --write \"**/*.{ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "bun run --bun eslint ./src",
    "lint:fix": "bun run --bun eslint ./src --fix",
    "typecheck": "bun run --bun tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@trpc/client": "^11.8.0",
    "@trpc/next": "^11.8.0",
    "@trpc/react-query": "^11.8.0",
    "@trpc/server": "^11.8.0",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.7",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "ioredis": "^5.8.2",
    "next": "^16.1.0",
    "postgres": "^3.4.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "sonner": "^2.0.7",
    "superjson": "^2.2.6",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@yopem/eslint-config": "^0.3.1",
    "@yopem/prettier-config": "^0.1.9",
    "@yopem/typescript-config": "^0.2.1",
    "drizzle-kit": "^0.31.8",
    "typescript": "~5.9.3",
    "vite": "^7.3.0"
  },
  "prettier": "@yopem/prettier-config/react",
  "packageManager": "bun@1.3.4"
